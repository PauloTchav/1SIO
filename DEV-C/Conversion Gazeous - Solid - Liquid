#include <stdio.h>
#include <stdlib.h>

int main()
{
    float eau = 0;
    printf("Rentrez la temperature de l'eau ?\n");
    scanf("%f", &eau);
    if (eau < 0) {
        printf("L'eau est solide\n");
    }
    if (eau >= 0 && eau < 100){
        printf("L'eau est liquide\n");
    }
    if (eau > 100){
    printf("L'eau est gazeuse\n");
    }

    return 0;
}
